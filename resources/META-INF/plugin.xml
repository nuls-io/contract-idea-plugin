<idea-plugin>
  <id>org.river.nuls.contract.tool</id>
  <name>Nuls</name>
  <version>1.0</version>
  <vendor email="support@cqfc.com" url="http://www.cqfc.org">CQFC</vendor>

  <description><![CDATA[
      Nuls Interface Manager<br>
      <em>Nuls Interface Manager</em>
    ]]></description>

  <change-notes><![CDATA[
      V1.0<br>
      <em>V1.0 released.</em>
    ]]>
  </change-notes>

  <idea-version since-build="173.0"/>

  <extensions defaultExtensionNs="com.intellij">
    <annotator language="JAVA" implementationClass="org.river.nuls.annotator.NulsAnnotator"/>
    <toolWindow id="Nuls" icon="/icons/nuls.png" anchor="right" factoryClass="org.river.nuls.toolwindow.NulsToolWindowFactory"/>
    <projectService serviceInterface="org.river.nuls.model.NulsConfiguration" serviceImplementation="org.river.nuls.model.NulsConfiguration"/>
    <projectService serviceInterface="org.river.nuls.logic.TreeItemManager" serviceImplementation="org.river.nuls.logic.TreeItemManager"/>
    <projectService serviceInterface="org.river.nuls.logic.LogManager" serviceImplementation="org.river.nuls.logic.LogManager"/>
    <projectService serviceInterface="org.river.nuls.logic.Notifier" serviceImplementation="org.river.nuls.logic.Notifier"/>
  </extensions>

  <actions>
    <group id="Nuls.AddGroupPopup"
           class="org.river.nuls.action.NulsActionGroup"
           text="Add"
           description="Add Account or Node"
           icon="AllIcons.General.Add">
    </group>
  </actions>

</idea-plugin>